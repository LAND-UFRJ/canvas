# API Keys
VITE_ANTHROPIC_API_KEY=your_anthropic_api_key
VITE_OPENAI_API_KEY=your_openai_api_key

# API Endpoints
VITE_ANTHROPIC_API_URL=https://api.anthropic.com
VITE_OPENAI_API_URL=https://api.openai.com

# MCP Registry
VITE_MCP_REGISTRY_URL=http://localhost/api/v1

# Default API to use (anthropic or openai)
VITE_DEFAULT_API=anthropic

# Default model to use
VITE_DEFAULT_ANTHROPIC_MODEL=claude-3-7-sonnet-20250219
VITE_DEFAULT_OPENAI_MODEL=gpt-4o

# Feature flags
VITE_ENABLE_MCP_DISCOVERY=true
VITE_ENABLE_TOOL_SUGGESTIONS=true

# Logging
VITE_LOG_LEVEL=info